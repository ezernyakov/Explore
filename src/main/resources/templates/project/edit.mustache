<!DOCTYPE HTML>
<html>

<head>
    <title>Explore It - Edit Project</title>
    {{>parts/meta}}
</head>

<body>
{{>parts/menu}}

<div class="container">
    <div class=mt-5>
        <form method="post" action="/project/list">
            <h2 class="text-primary">Project id:</h2><br>
                <input type="text" name="id" size="10" readonly="readonly" value="{{project.id}}"/>
                <h2 class="text-primary">Project name:</h2><br>
                <input type="text" name="name" size="60" value="{{project.name}}"/>
                <h2 class="text-primary">Project description:</h2><br>
                <input type="text" name="description" size="100" value="{{project.description}}"/>
            <button type="submit">Save</button>

        </form>
    </div>
</div>

{{>parts/scripts}}
</body>

</html>